<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Faireparti" table="FaireParti">
    <indexes>
      <index name="idEtudiant" columns="idEtudiant,idPromotion"/>
      <index name="idPromotion" columns="idPromotion"/>
      <index name="IDX_449DE89022DD08B8" columns="idEtudiant"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idetudiant" target-entity="Etudiant">
      <join-columns>
        <join-column name="idEtudiant" referenced-column-name="idUtilisateur"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idpromotion" target-entity="Promotion">
      <join-columns>
        <join-column name="idPromotion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
